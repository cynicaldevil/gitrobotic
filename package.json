{
  "name": "gitrobotic",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "electron": "1.2.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./",
    "package": "node packageApp.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "rebuild": "npm rebuild --runtime=electron --target=1.2.1 --disturl=https://atom.io/download/atom-shell --build-from-source",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "hot-server": "node -r babel-register server.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.5.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "concurrently": "^2.2.0",
    "cross-env": "^1.0.8",
    "electron-devtools-installer": "^2.0.0",
    "electron-packager": "^7.7.0",
    "electron-rebuild": "^1.1.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "node-loader": "^0.5.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "react-tap-event-plugin": "^1.0.0",
    "electron-debug": "^1.0.1",
    "electron-prebuilt": "^1.2.1",
    "express": "^4.14.0",
    "material-ui": "^0.15.4",
    "nodegit": "^0.14.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "react-tap-event-plugin": "^1.0.0",
    "source-map-support": "^0.4.2",
    "watch": "^0.19.1"
  }
}
